@using MVC_EntityLayer.Concrete
@model List<About>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Hakkımda 1</th>
        <th>Hakkımda 2</th>
        <th>Hakkımda Görsel 1</th>
        <th>Hakkımda Görsel 2</th>
    </tr>
    @foreach(var item in Model)
    {
        <tr>
            <th>@item.AboutId</th>
            <td>@item.AboutDetails1</td>
            <td>@item.AboutDetails2</td>
            <td>@item.AboutImage1</td>
            <td>@item.AboutImage2</td>
        </tr>
    }

</table>
@Html.Action("AboutPartial","About")